assume ds:data , cs:code

data segment
  list db 4h,5h,8h
  count equ 03h
  max db 01h dup (?)
data ends

code segment
start:
      mov ax,data
      mov ds,ax
      
      lea si,list
      mov al,[si]
      mov cl,count
      again:
            cmp al,[si+1]
            jnl next
            mov al,[si+1]
            
      next:
           dec cl
           inc si
           cmp cl,0h
           jg again
           
      mov bl,al
      mov ah,4ch
      mov al,0h
      int 03h
  code ends
  end start
      
