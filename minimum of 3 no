assume cs:code,ds:data

data segment
  list db 1h,6h,9h
  count equ 03h
  min db 01h dup (?)
data ends

code segment
start:
      mov ax,data
      mov ds,ax
      
      lea si,list
      mov al,[si]
      mov cl,count
      
      again:
             cmp al,[si-1]
             jnl prev
             mov al,[si-1]
       prev:
            dec si
            dec cl
            cmp cl,0h
            jc again
            
          mov bl,al
          mov ah,4ch
          mov al,0h
          int 21h
          
code ends
end start
